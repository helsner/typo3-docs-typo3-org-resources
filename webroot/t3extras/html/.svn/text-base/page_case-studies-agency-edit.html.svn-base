				<h1>Edit/Create Agency Profile</h1>

				<form action="#" method="post">
					<div class="b-form b-form-edit">
						<div class="b-form-row">
							<label for="form-name">Name</label>
							<div class="b-form-inputs">
								<input id="form-name" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-about-project">Description</label>
							<div class="b-form-inputs">
								<textarea id="form-about-project"></textarea>
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-about-project">Upload logo</label>
							<div class="b-form-inputs">
								<div class="b-form-file">
									<input type="file" maxlength="1" class="multi-file" />
									<span class="form-file-main-text">Choose file</span> <span class="form-file-secondary-text">Limits: jpg/png, 600x480px, 500KB</span>
								</div><!-- .b-form-file -->
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-link">Link</label>
							<div class="b-form-inputs">
								<input id="form-link" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-category">Category</label>
							<div class="b-form-inputs">
								<select id="form-category">
									<option value="0">Corporate Site</option>
								</select>
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-industry">Industry</label>
							<div class="b-form-inputs">
								<select id="form-industry">
									<option value="0">NGO</option>
								</select>
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-tags">Tags</label>
							<div class="b-form-inputs">
								<input id="form-tags" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-address">Address</label>
							<div class="b-form-inputs">
								<input id="form-address" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-city">City</label>
							<div class="b-form-inputs">
								<input id="form-city" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-country">Country:</label>
							<div class="b-form-inputs">
								<select id="form-country">
									<option value="0">Germany</option>
								</select>
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->
						<div class="b-form-row">
							<label for="form-zip">Zip:</label>
							<div class="b-form-inputs">
								<input id="form-zip" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->	
						<div class="b-form-row">
							<label for="form-contact-person">Contact Person:</label>
							<div class="b-form-inputs">
								<input id="form-contact-person" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->	
						<div class="b-form-row">
							<label for="form-email">Email:</label>
							<div class="b-form-inputs">
								<input id="form-email" type="text" />
							</div><!-- b-form-inputs -->
						</div><!-- b-form-row -->	
